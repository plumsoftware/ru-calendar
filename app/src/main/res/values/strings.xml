<resources>
    <string name="app_name">Календарь</string>

    <string-array name="days_of_week">
        <item>Понедельник</item>
        <item>Вторник</item>
        <item>Среда</item>
        <item>Четверг</item>
        <item>Пятница</item>
        <item>Суббота</item>
        <item>Воскресенье</item>
    </string-array>

    <string-array name="days_of_week_short">
        <item>Пн</item>
        <item>Вт</item>
        <item>Ср</item>
        <item>Чт</item>
        <item>Пт</item>
        <item>Сб</item>
        <item>Вс</item>
    </string-array>
    <declare-styleable name="CustomCalendar">
        <attr name="month_year_format" format="enum">
            <enum name="three_letter_month__with_year" value="0" />
            <enum name="full_month__with_year" value="1" />
        </attr>
        <attr name="day_of_week_length" format="integer" />
        <attr name="row_height" format="dimension" />
        <attr name="day_of_week_start_from" format="enum">
            <enum name="воскресенье" value="0" />
            <enum name="понедельник" value="1" />
            <enum name="вторник" value="2" />
            <enum name="среда" value="3" />
            <enum name="четверг" value="4" />
            <enum name="пятница" value="5" />
            <enum name="суббота" value="6" />
        </attr>
        <attr name="left_button_src" format="reference" />
        <attr name="right_button_src" format="reference" />
    </declare-styleable>
</resources>